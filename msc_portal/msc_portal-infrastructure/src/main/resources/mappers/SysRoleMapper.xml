<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.msc.fix.lisa.repository.db.mapper.SysRoleMapper">


    <select id="getRoles" resultType="com.msc.fix.lisa.domain.entity.system.SysUserRole">
        select a.*,b.* from sys_user a,sys_user_role b where a.account = b.account and a.yn = 1 and b.yn = 1
    </select>
</mapper>
